material Sky
{
	technique
	{
		pass
		{
			lighting off
			depth_write off

			texture_unit
			{
				cubic_texture cloudy_noon.jpg separateUV
				tex_address_mode clamp
			}
		}
	}
}

material Ground
{
	technique
	{
		pass
		{
			texture_unit
			{
				texture dirt-grass.jpg
				scale 0.25 0.25
			}
		}
	}
}
